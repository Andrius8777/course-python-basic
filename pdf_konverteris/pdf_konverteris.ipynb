    "import PyPDF2\n",
    "\n",
    "def get_num_pages(pdf_path):\n",
    "    with open(pdf_path, \"rb\") as file:\n",
    "        pdf = PyPDF2.PdfFileReader(file)\n",
    "        return pdf.getNumPages()\n",
